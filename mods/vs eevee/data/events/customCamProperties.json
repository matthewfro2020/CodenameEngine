{
    "params": [
		{
            "name": "Tween Ease (ex: circ, quad, cube, quart)",
            "type": "DropDown('linear', 'back', 'bounce', 'circ', 'cube', 'elastic', 'expo', 'quad', 'quart', 'quint', 'sine', 'smoothStep', 'smootherStep')",
            "defaultValue": "quart"
        },
        {
            "name": "Tween Type (ex: In, Out, InOut)",
            "type": "DropDown('In', 'Out', 'InOut')",
            "defaultValue": "Out"
        },
        {
            "name": "Time (in steps)",
            "type": "Float(0.01,1000,1,100)",
            "defaultValue": 1
        },
        {
            "name": "pos X (-1 to exclude)",
            "type": "Int(-10000,10000,1)",
            "defaultValue": 0
        },
        {
            "name": "pos Y (-1 to exclude)",
            "type": "Int(-10000,10000,1)",
            "defaultValue": 0
        },
        {
            "name": "zoom (-1 to exclude)",
            "type": "Float(-1,100,1,10)",
            "defaultValue": 1
        },
        {
            "name": "numbers are additive? (if it ADDS the above values to the current values",
            "type": "Bool",
            "defaultValue": false
        },
        {
            "name": "postEvent cam lerp ( only works if the below is true )",
            "type": "Float(0,5,1,100)",
            "defaultValue": 0.04
        },
        {
            "name": "return lerp?",
            "type": "Bool",
            "defaultValue": true
        }
    ]
}